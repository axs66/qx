- (void)handleDoubleTap:(UITapGestureRecognizer *)gesture {
    if (gesture.state != UIGestureRecognizerStateRecognized) return;

    // æ£€æŸ¥å¼€å…³çŠ¶æ€
    if (![[NSUserDefaults standardUserDefaults] boolForKey:@"DYYYEnableDoubleTapDownload"]) return;

    // æ˜¾ç¤ºä¸‹è½½æç¤º
    UIAlertController *alert = [UIAlertController alertControllerWithTitle:@"æç¤º" message:@"æ­£åœ¨ä¸‹è½½è§†é¢‘..." preferredStyle:UIAlertControllerStyleAlert];
    [self presentViewController:alert animated:YES completion:nil];

    // èŽ·å–è§†é¢‘ URL
    NSString *videoURLString = [self getVideoURL];
    if (!videoURLString) {
        NSLog(@"[DYYY] èŽ·å–è§†é¢‘åœ°å€å¤±è´¥");
        alert.message = @"èŽ·å–è§†é¢‘åœ°å€å¤±è´¥";
        dispatch_after(dispatch_time(DISPATCH_TIME_NOW, 1 * NSEC_PER_SEC), dispatch_get_main_queue(), ^{
            [alert dismissViewControllerAnimated:YES completion:nil];
        });
        return;
    }
    NSLog(@"[DYYY] è§†é¢‘ URL: %@", videoURLString);

    NSURL *videoURL = [NSURL URLWithString:videoURLString];
    NSURL *tempPath = [NSURL fileURLWithPath:[NSTemporaryDirectory() stringByAppendingPathComponent:@"tempVideo.mp4"]];

    // ä½¿ç”¨ NSURLSession ä¸‹è½½è§†é¢‘
    NSURLSession *session = [NSURLSession sharedSession];
    NSURLSessionDownloadTask *task = [session downloadTaskWithURL:videoURL completionHandler:^(NSURL *location, NSURLResponse *response, NSError *error) {
        if (error || !location) {
            NSLog(@"[DYYY] ä¸‹è½½å¤±è´¥: %@", error.localizedDescription);
            dispatch_async(dispatch_get_main_queue(), ^{
                alert.message = @"ä¸‹è½½å¤±è´¥ï¼Œè¯·æ£€æŸ¥ç½‘ç»œæˆ–URLæœ‰æ•ˆæ€§";
                dispatch_after(dispatch_time(DISPATCH_TIME_NOW, 1 * NSEC_PER_SEC), dispatch_get_main_queue(), ^{
                    [alert dismissViewControllerAnimated:YES completion:nil];
                });
            });
            return;
        }

        // ç§»åŠ¨ä¸‹è½½æ–‡ä»¶åˆ°ä¸´æ—¶è·¯å¾„
        NSError *moveError;
        [[NSFileManager defaultManager] moveItemAtURL:location toURL:tempPath error:&moveError];
        if (moveError) {
            NSLog(@"[DYYY] æ–‡ä»¶ç§»åŠ¨å¤±è´¥: %@", moveError.localizedDescription);
            dispatch_async(dispatch_get_main_queue(), ^{
                alert.message = @"æ–‡ä»¶ç§»åŠ¨å¤±è´¥";
                dispatch_after(dispatch_time(DISPATCH_TIME_NOW, 1 * NSEC_PER_SEC), dispatch_get_main_queue(), ^{
                    [alert dismissViewControllerAnimated:YES completion:nil];
                });
            });
            return;
        }

        // ä¿å­˜è§†é¢‘åˆ°ç›¸å†Œ
        [PHPhotoLibrary requestAuthorization:^(PHAuthorizationStatus status) {
            if (status == PHAuthorizationStatusAuthorized) {
                [[PHPhotoLibrary sharedPhotoLibrary] performChanges:^{
                    [PHAssetCreationRequest creationRequestForAssetFromVideoAtFileURL:tempPath];
                } completionHandler:^(BOOL success, NSError *error) {
                    dispatch_async(dispatch_get_main_queue(), ^{
                        if (success) {
                            NSLog(@"[DYYY] è§†é¢‘ä¿å­˜æˆåŠŸ");
                            alert.message = @"ä¸‹è½½å®Œæˆï¼Œå·²ä¿å­˜åˆ°ç›¸å†Œ";
                        } else {
                            NSLog(@"[DYYY] ä¿å­˜åˆ°ç›¸å†Œå¤±è´¥: %@", error.localizedDescription);
                            alert.message = @"ä¿å­˜åˆ°ç›¸å†Œå¤±è´¥";
                        }
                        dispatch_after(dispatch_time(DISPATCH_TIME_NOW, 1 * NSEC_PER_SEC), dispatch_get_main_queue(), ^{
                            [alert dismissViewControllerAnimated:YES completion:nil];
                        });
                    });
                }];
            } else {
                NSLog(@"[DYYY] æ— ç›¸å†Œæƒé™");
                dispatch_async(dispatch_get_main_queue(), ^{
                    alert.message = @"æ— ç›¸å†Œæƒé™ï¼Œè¯·åœ¨è®¾ç½®ä¸­å¼€å¯";
                    dispatch_after(dispatch_time(DISPATCH_TIME_NOW, 1 * NSEC_PER_SEC), dispatch_get_main_queue(), ^{
                        [alert dismissViewControllerAnimated:YES completion:nil];
                    });
