<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>name</key>
    <string>Cursor</string>
    <key>description</key>
    <string>Cursor AI Editor Rules</string>
    <key>rules</key>
    <array>
        <!-- Cursor 主域名 -->
        <dict>
            <key>action</key>
            <string>REJECT</string>
            <key>pattern</key>
            <string>^https?:\/\/(.*\.)?cursor\.com</string>
        </dict>
        <dict>
            <key>action</key>
            <string>REJECT</string>
            <key>pattern</key>
            <string>^https?:\/\/(.*\.)?cursor\.sh</string>
        </dict>
        
        <!-- GitHub Copilot (Cursor 依赖) -->
        <dict>
            <key>action</key>
            <string>REJECT</string>
            <key>pattern</key>
            <string>^https?:\/\/(.*\.)?githubcopilot\.com</string>
        </dict>
        <dict>
            <key>action</key>
            <string>REJECT</string>
            <key>pattern</key>
            <string>^https?:\/\/copilot-proxy\.githubusercontent\.com</string>
        </dict>
        
        <!-- AI 服务提供商 -->
        <dict>
            <key>action</key>
            <string>REJECT</string>
            <key>pattern</key>
            <string>^https?:\/\/(.*\.)?openai\.com</string>
        </dict>
        <dict>
            <key>action</key>
            <string>REJECT</string>
            <key>pattern</key>
            <string>^https?:\/\/(.*\.)?anthropic\.com</string>
        </dict>
        <dict>
            <key>action</key>
            <string>REJECT</string>
            <key>pattern</key>
            <string>^https?:\/\/(.*\.)?claude\.ai</string>
        </dict>
        <dict>
            <key>action</key>
            <string>REJECT</string>
            <key>pattern</key>
            <string>^https?:\/\/(.*\.)?claudeusercontent\.com</string>
        </dict>
        
        <!-- 其他 AI 服务 -->
        <dict>
            <key>action</key>
            <string>REJECT</string>
            <key>pattern</key>
            <string>^https?:\/\/(.*\.)?groq\.com</string>
        </dict>
        <dict>
            <key>action</key>
            <string>REJECT</string>
            <key>pattern</key>
            <string>^https?:\/\/(.*\.)?together\.xyz</string>
        </dict>
    </array>
</dict>
</plist>
